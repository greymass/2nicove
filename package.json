{
	"name": "unicove",
	"version": "2.0.0-alpha",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "rm -rf ./.svelte-kit ./node_modules ./src/paraglide",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.11.2",
		"@inlang/paraglide-sveltekit": "0.11.0",
		"@inlang/paraglide-vite": "^1.2.74",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-cloudflare": "^4.6.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/big.js": "^6.2.2",
		"@types/eslint": "^8.56.10",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"globals": "^15.8.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.5",
		"svelte": "^5.0.0-next.184",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.0-alpha.44",
		"vite": "^5.3.3"
	},
	"type": "module",
	"dependencies": {
		"@wharfkit/antelope": "^1.0.9-rc1",
		"@wharfkit/contract": "^1.1.5",
		"@wharfkit/session": "^1.3.1",
		"@wharfkit/wallet-plugin-anchor": "^1.3.5",
		"@wharfkit/wallet-plugin-privatekey": "^1.1.0",
		"@wharfkit/web-renderer": "^1.4.0-rc6",
		"big.js": "^6.2.1",
		"just-extend": "^6.2.0",
		"svead": "^0.0.11"
	}
}
